  "homepage": "https://www.linkedin.com/in/barahalikar-siddharth/",
  "license": "MIT",
  "scripts": {
    "start": "node app.js",
    "test": "mocha app-test.js --timeout 10000 --reporter mocha-junit-reporter --exit",
    "coverage": "nyc --cwd=. --report-dir=coverage --reporter=lcov --reporter=text --reporter=cobertura --reporter=json-summary mocha app-test.js --timeout 10000 --exit"
  },
  "nyc": {
    "all": true,
    "include": ["app.js"],
    "extension": [".js"],
    "report-dir": "coverage",
    "exclude": [
      "node_modules/**",
      "**/*test.js"
    ]
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "mocha-junit-reporter": "^2.2.1",
    "mongoose": "^8.18.0",
    "nyc": "^15.1.0",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "chai": "*",
    "chai-http": "*",
    "mocha": "*"
  }
}
